        <!-- Footer -->
        <footer class="footer skewed">
            <span id="contact" class="navpoint"></span>
            <div class="section-title inverted">
                <h3>Contact</h3>
            </div>

            <div class="forced-width">
                <p>
                    If you want to contact me, please use the form below or send
                    me an email:
                    <a href="mailto:ahlgreenmadsen@gmail.com"
                        >ahlgreenmadsen@gmail.com</a
                    >
                </p>

                <div class="tight-forced-width form-container">
                    <form class="contact-form">
                        <div class="input-container">
                            <input
                                type="text"
                                name="email"
                                id="contact-email"
                                placeholder=""
                                data-pattern-type="^([a-zA-Z0-9_\-\.]+)@([a-zA-Z0-9_\-\.]+)\.([a-zA-Z]{2,5})$"
                                required
                            />
                            <label for="contact-email">Email</label>
                        </div>

                        <div class="flex">
                            <div class="input-container">
                                <input
                                    type="text"
                                    name="name"
                                    id="contact-name"
                                    placeholder=""
                                    required
                                />
                                <label for="contact-name">Name</label>
                            </div>

                            <div class="input-container">
                                <input
                                    type="text"
                                    name="subject"
                                    id="contact-subject"
                                    placeholder=""
                                    required
                                />
                                <label for="contact-subject">Subject</label>
                            </div>
                        </div>

                        <div class="input-container">
                            <textarea
                                name="message"
                                id="contact-message"
                                placeholder=""
                                required
                            ></textarea>
                            <label for="contact-message">Message</label>
                        </div>

                        <button id="send-message-btn">Send Message</button>
                    </form>

                    <section class="form-submitted">
                        The form has been successfully submitted.
                    </section>
                </div>
